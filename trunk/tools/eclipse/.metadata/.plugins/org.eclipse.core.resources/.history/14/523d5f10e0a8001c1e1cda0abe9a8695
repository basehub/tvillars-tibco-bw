<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns1="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/mail" xmlns:pfx="http://xmlns.tibco.com/body" xmlns:ns3="http://www.tibco.com/pe/EngineTypes" xmlns:ns4="www.tibco.com/plugin/Sleep" xmlns:ns42="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx2="http://tvillars-tibco-bw.googlecode.com/svn/trunk/src/bizworks/Common/Common/Processes/Notify.xsd">
