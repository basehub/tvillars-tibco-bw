    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="FTP.Input">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:choice>
                        <xsd:element name="Dir">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="DirParameter" type="xsd:string" minOccurs="0"/>
