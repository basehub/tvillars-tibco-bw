<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:guid="java://com.chicos.eai.common.functions.GUID" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:ns2="http://code.google.com/tvillars/tibco/bw/Common/Schemas/Common" xmlns:ns0="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns3="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" xmlns:ns32="http://www.tibco.com/namespaces/tnt/plugins/file/v2">
    <xsd:import namespace="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" schemaLocation="/Common/Functions/GEH.xsd"/>
    <pd:name>Common/Functions/UnitTest/GEH/Handled TechError - Retry Forever.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Start">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="FileName" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>58</pd:startX>
    <pd:startY>156</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>551</pd:endX>
    <pd:endY>154</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:group name="read file - If error, log, pause and retry">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>138</pd:x>
        <pd:y>1</pd:y>
        <pd:width>312</pd:width>
        <pd:height>290</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <serializable>false</serializable>
            <pd:indexSlot>retryCount</pd:indexSlot>
            <pd:errorCondition>false()</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Read File</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>GEH - Read File</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65536</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:activity name="GEH - Read File">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>292</pd:x>
            <pd:y>210</pd:y>
            <config>
                <processName>/Common/Functions/GEH.process</processName>
            </config>
            <pd:inputBindings>
                <GEH>
                    <xsl:copy-of select="$_error/ns:ErrorReport"/>
                    <ns2:Context>
                        <ns2:ProcessInput>
                            <xsl:copy-of select="$Start/Start"/>
                        </ns2:ProcessInput>
                    </ns2:Context>
                    <Retry>
                        <RetryCount>
                            <xsl:value-of select="$retryCount"/>
                        </RetryCount>
                        <ns3:RetryOptions>
                            <ns3:TryForever>
                                <ns3:FirstError>
                                    <ns3:SecsToPauseBetweenRetries>
                                        <xsl:value-of select="2"/>
                                    </ns3:SecsToPauseBetweenRetries>
                                    <ns3:RetryCountToLogError>
                                        <xsl:value-of select="3"/>
                                    </ns3:RetryCountToLogError>
                                </ns3:FirstError>
                                <ns3:SubsequentErrors>
                                    <ns3:SecsToPauseBetweenRetries>
                                        <xsl:value-of select="10"/>
                                    </ns3:SecsToPauseBetweenRetries>
                                    <ns3:RetryCountToLogError>
                                        <xsl:value-of select="3"/>
                                    </ns3:RetryCountToLogError>
                                </ns3:SubsequentErrors>
                            </ns3:TryForever>
                        </ns3:RetryOptions>
                    </Retry>
                </GEH>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Read File">
            <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
            <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
            <pd:x>288</pd:x>
            <pd:y>67</pd:y>
            <config>
                <encoding>text</encoding>
            </config>
            <pd:inputBindings>
                <ns32:ReadActivityInputClass>
                    <fileName>
                        <xsl:value-of select="$Start/Start/FileName"/>
                    </fileName>
                </ns32:ReadActivityInputClass>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>read file - If error, log, pause and retry</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Straight</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>read file - If error, log, pause and retry</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>