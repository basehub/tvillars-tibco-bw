<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:guid="java://com.chicos.eai.common.functions.GUID" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:ns2="http://code.google.com/tvillars/tibco/bw/Common/Schemas/Common" xmlns:ns0="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx2="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH">
    <xsd:import namespace="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" schemaLocation="/Common/Functions/GEH.xsd"/>
    <pd:name>Common/Functions/UnitTest/GEH/Subprocesses/Handled DataError.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Unhandled-Error">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>126</pd:startX>
    <pd:startY>60</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>481</pd:endX>
    <pd:endY>61</pd:endY>
    <pd:errorSchemas>
        <DataError ref="pfx2:DataError"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:activity name="Handled DataError">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>286</pd:x>
        <pd:y>207</pd:y>
        <config>
            <faultName>DataError</faultName>
        </config>
        <pd:inputBindings>
            <ns0:ActivityInput>
                <data>
                    <pfx2:DataError>
                        <ns2:Context>
                            <ns2:ProcessInput>
                                <xsl:copy-of select="$Start/Unhandled-Error"/>
                            </ns2:ProcessInput>
                        </ns2:Context>
                        <pfx2:DescShort>
                            <xsl:value-of select="$_error/ns:ErrorReport/Msg"/>
                        </pfx2:DescShort>
                        <pfx2:ErrorReport>
                            <xsl:copy-of select="$_error/ns:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                            <xsl:copy-of select="$_error/ns:ErrorReport/@*"/>
                            <xsl:copy-of select="$_error/ns:ErrorReport/node()"/>
                        </pfx2:ErrorReport>
                    </pfx2:DataError>
                </data>
            </ns0:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>288</pd:x>
        <pd:y>62</pd:y>
        <config>
            <element>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="param" type="xsd:int"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <root>
                <param>
                    <xsl:value-of select="guid:makeGUID()"/>
                </param>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Handled DataError</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>