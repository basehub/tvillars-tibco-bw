<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns2="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH">
    <pd:name>Common/Functions/UnitTest/FileDelUsingOS.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>52</pd:startX>
    <pd:startY>165</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>739</pd:endX>
    <pd:endY>165</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:group name="create 4 files called Test{index}.txt">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>118</pd:x>
        <pd:y>46</pd:y>
        <pd:width>234</pd:width>
        <pd:height>223</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <serializable>false</serializable>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:repeatCondition>$index = 4</pd:repeatCondition>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Write File</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Write File</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Write File">
            <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
            <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
            <pd:x>236</pd:x>
            <pd:y>124</pd:y>
            <config>
                <encoding>text</encoding>
                <compressFile>None</compressFile>
                <createMissingDirectories>true</createMissingDirectories>
            </config>
            <pd:inputBindings>
                <ns0:WriteActivityInputTextClass>
                    <fileName>
                        <xsl:value-of select="concat('c:\temp\test', $index,'.txt')"/>
                    </fileName>
                    <textContent>
                        <xsl:value-of select="'Test Data'"/>
                    </textContent>
                </ns0:WriteActivityInputTextClass>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Remove Files From Directory That Does Not Exist">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>529</pd:x>
        <pd:y>180</pd:y>
        <config>
            <processName>/Common/Functions/FileDelUsingOS.process</processName>
        </config>
        <pd:inputBindings>
            <FileDelUsingOS>
                <fileName>
                    <xsl:value-of select="'c:\temp\testxxxxx\*.txt'"/>
                </fileName>
                <ns2:RetryOptions>
                    <ns2:TryXTimes>
                        <ns2:TimesToTry>
                            <xsl:value-of select="2"/>
                        </ns2:TimesToTry>
                        <ns2:SecsToPauseBetweenRetries>
                            <xsl:value-of select="1"/>
                        </ns2:SecsToPauseBetweenRetries>
                    </ns2:TryXTimes>
                </ns2:RetryOptions>
            </FileDelUsingOS>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Remove Files That Do Not Exist">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>529</pd:x>
        <pd:y>323</pd:y>
        <config>
            <processName>/Common/Functions/FileDelUsingOS.process</processName>
        </config>
        <pd:inputBindings>
            <FileDelUsingOS>
                <fileName>
                    <xsl:value-of select="'c:\temp\test*.xxx'"/>
                </fileName>
                <ns2:RetryOptions>
                    <ns2:TryXTimes>
                        <ns2:TimesToTry>
                            <xsl:value-of select="2"/>
                        </ns2:TimesToTry>
                        <ns2:SecsToPauseBetweenRetries>
                            <xsl:value-of select="1"/>
                        </ns2:SecsToPauseBetweenRetries>
                    </ns2:TryXTimes>
                </ns2:RetryOptions>
            </FileDelUsingOS>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Remove Files That Exists">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>529</pd:x>
        <pd:y>54</pd:y>
        <config>
            <processName>/Common/Functions/FileDelUsingOS.process</processName>
        </config>
        <pd:inputBindings>
            <FileDelUsingOS>
                <fileName>
                    <xsl:value-of select="'c:\temp\test*.txt'"/>
                </fileName>
                <ns2:RetryOptions>
                    <ns2:TryForever/>
                </ns2:RetryOptions>
            </FileDelUsingOS>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>create 4 files called Test{index}.txt</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>create 4 files called Test{index}.txt</pd:from>
        <pd:to>Remove Files That Exists</pd:to>
        <pd:lineType>Curved</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Remove Files That Exists</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Remove Files That Do Not Exist</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>create 4 files called Test{index}.txt</pd:from>
        <pd:to>Remove Files From Directory That Does Not Exist</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Remove Files From Directory That Does Not Exist</pd:from>
        <pd:to>Remove Files That Do Not Exist</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Remove Files From Directory That Does Not Exist</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>