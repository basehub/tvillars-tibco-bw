<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" xmlns:guid="java://com.chicos.eai.common.functions.GUID" xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://code.google.com/tvillars/tibco/bw/Common/Schemas/Common" xmlns:ns4="http://www.tibco.com/pe/DeployedVarsType">
    <pd:name>Common/Functions/UnitTest/FileCopy.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>43</pd:startX>
    <pd:startY>45</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>539</pd:endX>
    <pd:endY>45</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:activity name="Copy A File That Exist">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>272</pd:x>
        <pd:y>53</pd:y>
        <config>
            <processName>/Common/Functions/FileCopy.process</processName>
        </config>
        <pd:inputBindings>
            <FileCopy>
                <sourceFullFileName>
                    <xsl:value-of select="$Write-File/ns1:WriteActivityOutputClass/fileInfo/fullName"/>
                </sourceFullFileName>
                <targetFullFileName>
                    <xsl:value-of select="concat($Write-File/ns1:WriteActivityOutputClass/fileInfo/fullName, '.copy')"/>
                </targetFullFileName>
                <ns:RetryOptions>
                    <ns:TryForever/>
                </ns:RetryOptions>
            </FileCopy>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="GEH">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>415</pd:x>
        <pd:y>199</pd:y>
        <config>
            <processName>/Common/Functions/GEH.process</processName>
        </config>
        <pd:inputBindings>
            <GEH>
                <xsl:copy-of select="$_error/ns2:ErrorReport"/>
            </GEH>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Try To Copy A File That Does Not Exists">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>415</pd:x>
        <pd:y>60</pd:y>
        <config>
            <processName>/Common/Functions/FileCopy.process</processName>
        </config>
        <pd:inputBindings>
            <FileCopy>
                <sourceFullFileName>
                    <xsl:value-of select="concat('c:\temp\', guid:makeGUID(),'.txt')"/>
                </sourceFullFileName>
                <targetFullFileName>
                    <xsl:value-of select="'c:\temp\missing.txt'"/>
                </targetFullFileName>
                <AbortIfFileNotFound>
                    <xsl:value-of select="true()"/>
                </AbortIfFileNotFound>
                <ns:RetryOptions>
                    <ns:TryForever/>
                </ns:RetryOptions>
            </FileCopy>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Write File">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>159</pd:x>
        <pd:y>45</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
            <createMissingDirectories>true</createMissingDirectories>
        </config>
        <pd:inputBindings>
            <ns1:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="concat($_globalVariables/ns4:GlobalVariables/Constants/Technical/TempDir, '/test1.txt')"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="'Test Data'"/>
                </textContent>
            </ns1:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Write File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Write File</pd:from>
        <pd:to>Copy A File That Exist</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Copy A File That Exist</pd:from>
        <pd:to>Try To Copy A File That Does Not Exists</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Try To Copy A File That Does Not Exists</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Try To Copy A File That Does Not Exists</pd:from>
        <pd:to>GEH</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GEH</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>