<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" xmlns:guid="java://com.chicos.eai.common.functions.GUID" xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://uscis.gov/uscis/xsd/services/common/1.0">
    <pd:name>Common/Functions/UnitTest/FileCreate.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>41</pd:startX>
    <pd:startY>44</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>427</pd:endX>
    <pd:endY>44</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:activity name="Create A File">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>160</pd:x>
        <pd:y>44</pd:y>
        <config>
            <processName>/Common/Functions/FileCreate.process</processName>
        </config>
        <pd:inputBindings>
            <FileCreate>
                <FullFileName>
                    <xsl:value-of select="'c:\temp\FileCreateTest.txt'"/>
                </FullFileName>
                <ns:RetryOptions>
                    <ns:TryForever/>
                </ns:RetryOptions>
            </FileCreate>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="GEH">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>291</pd:x>
        <pd:y>210</pd:y>
        <config>
            <processName>/Common/Functions/GEH.process</processName>
        </config>
        <pd:inputBindings>
            <GEH>
                <xsl:copy-of select="$_error/ns2:ErrorReport"/>
            </GEH>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Try To Create A File On A Drive That Does Not Exist">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>291</pd:x>
        <pd:y>67</pd:y>
        <config>
            <processName>/Common/Functions/FileCreate.process</processName>
        </config>
        <pd:inputBindings>
            <FileCreate>
                <FullFileName>
                    <xsl:value-of select="'ZZ:\NoSuchDrive.txt'"/>
                </FullFileName>
                <ns:RetryOptions>
                    <ns:TryXTimes>
                        <ns:TimesToTry>
                            <xsl:value-of select="2"/>
                        </ns:TimesToTry>
                        <ns:SecsToPauseBetweenRetries>
                            <xsl:value-of select="1"/>
                        </ns:SecsToPauseBetweenRetries>
                    </ns:TryXTimes>
                </ns:RetryOptions>
            </FileCreate>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Create A File</pd:from>
        <pd:to>Try To Create A File On A Drive That Does Not Exist</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Try To Create A File On A Drive That Does Not Exist</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Try To Create A File On A Drive That Does Not Exist</pd:from>
        <pd:to>GEH</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GEH</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Create A File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>