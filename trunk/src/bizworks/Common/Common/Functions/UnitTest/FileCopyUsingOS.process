<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.chicos.com/eai/Common/Functions/GEH" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file">
    <pd:name>Common/Functions/UnitTest/FileCopyUsingOS.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>81</pd:startX>
    <pd:startY>124</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>663</pd:endX>
    <pd:endY>126</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:group name="create 2 files called Test{index}.txt">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>155</pd:x>
        <pd:y>18</pd:y>
        <pd:width>239</pd:width>
        <pd:height>200</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <serializable>false</serializable>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:repeatCondition>$index = 2</pd:repeatCondition>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Write File</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Write File</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Write File">
            <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
            <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
            <pd:x>272</pd:x>
            <pd:y>83</pd:y>
            <config>
                <encoding>text</encoding>
                <compressFile>None</compressFile>
                <createMissingDirectories>true</createMissingDirectories>
            </config>
            <pd:inputBindings>
                <ns1:WriteActivityInputTextClass>
                    <fileName>
                        <xsl:value-of select="concat('c:\temp\test', $index,'.txt')"/>
                    </fileName>
                    <textContent>
                        <xsl:value-of select="'Test Data'"/>
                    </textContent>
                </ns1:WriteActivityInputTextClass>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Copy File Using OS (Text1.txt)">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>515</pd:x>
        <pd:y>49</pd:y>
        <config>
            <processName>/Common/Functions/FileCopyUsingOS.process</processName>
        </config>
        <pd:inputBindings>
            <FileCopyUsingOS>
                <source>
                    <xsl:value-of select="'c:\temp\test1.txt'"/>
                </source>
                <target>
                    <xsl:value-of select="'c:\temp\test1.txt.copy'"/>
                </target>
                <ns:RetryOptions>
                    <ns:TryForever/>
                </ns:RetryOptions>
            </FileCopyUsingOS>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Copy File Using OS (Text2.txt)">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>516</pd:x>
        <pd:y>134</pd:y>
        <config>
            <processName>/Common/Functions/FileCopyUsingOS.process</processName>
        </config>
        <pd:inputBindings>
            <FileCopyUsingOS>
                <source>
                    <xsl:value-of select="'test2.txt'"/>
                </source>
                <target>
                    <xsl:value-of select="'test2.txt.copy'"/>
                </target>
                <workingDirectory>
                    <xsl:value-of select="'c:/temp'"/>
                </workingDirectory>
                <ns:RetryOptions>
                    <ns:TryForever/>
                </ns:RetryOptions>
            </FileCopyUsingOS>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Should Error And Go Here">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>515</pd:x>
        <pd:y>357</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Try To Copy A Missing File Using OS (Missing.txt)">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>517</pd:x>
        <pd:y>229</pd:y>
        <config>
            <processName>/Common/Functions/FileCopyUsingOS.process</processName>
        </config>
        <pd:inputBindings>
            <FileCopyUsingOS>
                <source>
                    <xsl:value-of select="'Z:missing.txt'"/>
                </source>
                <target>
                    <xsl:value-of select="'missingA.txt'"/>
                </target>
                <workingDirectory>
                    <xsl:value-of select="'c:\temp'"/>
                </workingDirectory>
                <ns:RetryOptions>
                    <ns:TryXTimes>
                        <ns:TimesToTry>
                            <xsl:value-of select="3"/>
                        </ns:TimesToTry>
                        <ns:SecsToPauseBetweenRetries>
                            <xsl:value-of select="2"/>
                        </ns:SecsToPauseBetweenRetries>
                    </ns:TryXTimes>
                </ns:RetryOptions>
            </FileCopyUsingOS>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>create 2 files called Test{index}.txt</pd:from>
        <pd:to>Copy File Using OS (Text1.txt)</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Copy File Using OS (Text1.txt)</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>create 2 files called Test{index}.txt</pd:from>
        <pd:to>Copy File Using OS (Text2.txt)</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Copy File Using OS (Text2.txt)</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>create 2 files called Test{index}.txt</pd:from>
        <pd:to>Try To Copy A Missing File Using OS (Missing.txt)</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Try To Copy A Missing File Using OS (Missing.txt)</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>create 2 files called Test{index}.txt</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Try To Copy A Missing File Using OS (Missing.txt)</pd:from>
        <pd:to>Should Error And Go Here</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Should Error And Go Here</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>