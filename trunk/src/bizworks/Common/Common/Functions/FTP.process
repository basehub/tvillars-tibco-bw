<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns1="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://www.chicos.com/eai/Common/Schemas/Common" xmlns:ns22="http://www.chicos.com/eai/Common/Functions/GEH" xmlns:ns12="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <xsd:import namespace="http://www.chicos.com/eai/Common/Functions/GEH" schemaLocation="/Common/Functions/GEH.xsd"/>
    <pd:name>Common/Functions/FTP.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="FTP">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:choice>
                        <xsd:element name="Dir">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="DirParameter" type="xsd:string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Get">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="RemoteFileName" type="xsd:string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Put">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="Data" type="xsd:string" minOccurs="0"/>
                                    <xsd:element name="RemoteFileName" type="xsd:string"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Rename">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="OldRemoteDirectory" type="xsd:string" minOccurs="0"/>
                                    <xsd:element name="OldRemoteFileName" type="xsd:string"/>
                                    <xsd:element name="NewRemoteDirectory" type="xsd:string" minOccurs="0"/>
                                    <xsd:element name="NewRemoteFileName" type="xsd:string"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:choice>
                    <xsd:element name="Host" type="xsd:string"/>
                    <xsd:element name="Port" type="xsd:string"/>
                    <xsd:element name="Username" type="xsd:string"/>
                    <xsd:element name="Password" type="xsd:string"/>
                    <xsd:element ref="ns22:RetryOptions"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>323</pd:startX>
    <pd:startY>36</pd:startY>
    <pd:returnBindings>
        <root>
            <xsl:choose>
                <xsl:when test="exists($Start/FTP/Dir)">
                    <DirOutput>
                        <ItemCount>
                            <xsl:value-of select="$FTP-Dir/Output/ItemCount"/>
                        </ItemCount>
                        <xsl:for-each select="$FTP-Dir/Output/DirectoryItems">
                            <DirectoryItems>
                                <xsl:value-of select="."/>
                            </DirectoryItems>
                        </xsl:for-each>
                    </DirOutput>
                </xsl:when>
                <xsl:when test="exists($Start/FTP/Get)">
                    <GetOutput>
                        <FTPGetOutputASCII>
                            <ASCIIData>
                                <xsl:value-of select="$FTP-Get/FTPGetOutputASCII/ASCIIData"/>
                            </ASCIIData>
                        </FTPGetOutputASCII>
                        <xsl:copy-of select="$FTP-Get/FTPGetOutputASCII/FileTransferred"/>
                    </GetOutput>
                </xsl:when>
                <xsl:otherwise>
                    <PutOutput>
                        <xsl:copy-of select="$FTP-Put/FTPPutOutputFile/FileTransferred"/>
                    </PutOutput>
                </xsl:otherwise>
            </xsl:choose>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:choice>
                    <xsd:element name="DirOutput">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="ItemCount" type="xsd:int"/>
                                <xsd:element name="DirectoryItems" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="GetOutput">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="FTPGetOutputASCII">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="ASCIIData" type="xsd:string"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                                <xsd:element name="FileTransferred" minOccurs="0" maxOccurs="unbounded">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="Name" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="NumOfBytes" type="xsd:int" minOccurs="0"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element name="PutOutput">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="FileTransferred" minOccurs="0" maxOccurs="unbounded">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="Name" type="xsd:string" minOccurs="0"/>
                                            <xsd:element name="NumOfBytes" type="xsd:int" minOccurs="0"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:choice>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>642</pd:endX>
    <pd:endY>439</pd:endY>
    <pd:errorSchemas>
        <TechError ref="ns22:TechError"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:group name="FTP Operations">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>39</pd:x>
        <pd:y>87</pd:y>
        <pd:width>467</pd:width>
        <pd:height>437</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <serializable>false</serializable>
            <pd:indexSlot>retryCount</pd:indexSlot>
            <pd:errorCondition>false()</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Execute Operation</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Execute Operation</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>GEH - FTP Error</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Straight</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Execute Operation</pd:from>
            <pd:to>GEH - FTP Error</pd:to>
            <pd:lineType>Curved</pd:lineType>
            <pd:lineColor>-65536</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:group name="Execute Operation">
            <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
            <pd:resourceType>ae.process.group</pd:resourceType>
            <pd:x>82</pd:x>
            <pd:y>119</pd:y>
            <pd:width>227</pd:width>
            <pd:height>373</pd:height>
            <pd:collapsedWidth>57</pd:collapsedWidth>
            <pd:collapsedHeight>70</pd:collapsedHeight>
            <pd:groupVariables/>
            <pd:joinToggle>inherit</pd:joinToggle>
            <config>
                <pd:groupType>simpleGroup</pd:groupType>
                <serializable>false</serializable>
            </config>
            <pd:inputBindings/>
            <pd:expanded>true</pd:expanded>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>FTP Dir</pd:to>
                <pd:xpathDescription>DIR</pd:xpathDescription>
                <pd:lineType>Straight</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>xpath</pd:conditionType>
                <pd:xpath>exists($Start/FTP/Dir)</pd:xpath>
            </pd:transition>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>FTP Rename</pd:to>
                <pd:xpathDescription>RENAME</pd:xpathDescription>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>xpath</pd:conditionType>
                <pd:xpath>exists($Start/FTP/Rename)</pd:xpath>
            </pd:transition>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>FTP Get</pd:to>
                <pd:xpathDescription>GET</pd:xpathDescription>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>xpath</pd:conditionType>
                <pd:xpath>exists($Start/FTP/Get)</pd:xpath>
            </pd:transition>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>FTP Put</pd:to>
                <pd:xpathDescription>PUT</pd:xpathDescription>
                <pd:lineType>Straight</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>xpath</pd:conditionType>
                <pd:xpath>exists($Start/FTP/Put)</pd:xpath>
            </pd:transition>
            <pd:transition>
                <pd:from>FTP Put</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Straight</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>FTP Dir</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Straight</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>FTP Get</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>FTP Rename</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:activity name="FTP Dir">
                <pd:type>com.tibco.plugin.ftp.FTPDirActivity</pd:type>
                <pd:resourceType>ae.activities.FTPDirActivityUI</pd:resourceType>
                <pd:x>199</pd:x>
                <pd:y>181</pd:y>
                <config>
                    <SharedUserData>/Common/Functions/Dummy FTP Connection.sharedftp</SharedUserData>
                    <NLST>true</NLST>
                </config>
                <pd:inputBindings>
                    <Input>
                        <Host>
                            <xsl:value-of select="$Start/FTP/Host"/>
                        </Host>
                        <Port>
                            <xsl:value-of select="$Start/FTP/Port"/>
                        </Port>
                        <UserName>
                            <xsl:value-of select="$Start/FTP/Username"/>
                        </UserName>
                        <Password>
                            <xsl:value-of select="$Start/FTP/Password"/>
                        </Password>
                        <DirParameters>
                            <xsl:value-of select="$Start/FTP/Dir/DirParameter"/>
                        </DirParameters>
                    </Input>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="FTP Get">
                <pd:type>com.tibco.plugin.ftp.FTPGetActivity</pd:type>
                <pd:resourceType>ae.activities.FTPGetActivityUI</pd:resourceType>
                <pd:x>200</pd:x>
                <pd:y>261</pd:y>
                <config>
                    <Timeout>120000</Timeout>
                    <isBinary>false</isBinary>
                    <Overwrite>false</Overwrite>
                    <useProcessData>true</useProcessData>
                    <SharedUserData>/Common/Functions/Dummy FTP Connection.sharedftp</SharedUserData>
                </config>
                <pd:inputBindings>
                    <FTPGetInputData>
                        <RemoteFileName>
                            <xsl:value-of select="$Start/FTP/Get/RemoteFileName"/>
                        </RemoteFileName>
                        <Host>
                            <xsl:value-of select="$Start/FTP/Host"/>
                        </Host>
                        <Port>
                            <xsl:value-of select="$Start/FTP/Port"/>
                        </Port>
                        <UserName>
                            <xsl:value-of select="$Start/FTP/Username"/>
                        </UserName>
                        <Password>
                            <xsl:value-of select="$Start/FTP/Password"/>
                        </Password>
                    </FTPGetInputData>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="FTP Put">
                <pd:type>com.tibco.plugin.ftp.FTPPutActivity</pd:type>
                <pd:resourceType>ae.activities.FTPPutActivityUI</pd:resourceType>
                <pd:x>199</pd:x>
                <pd:y>340</pd:y>
                <config>
                    <Timeout>120000</Timeout>
                    <FireWall>false</FireWall>
                    <isBinary>false</isBinary>
                    <Append>false</Append>
                    <useProcessData>true</useProcessData>
                    <Overwrite>true</Overwrite>
                    <SharedUserData>/Common/Functions/Dummy FTP Connection.sharedftp</SharedUserData>
                </config>
                <pd:inputBindings>
                    <FTPPutInputDataASCII>
                        <RemoteFileName>
                            <xsl:value-of select="$Start/FTP/Put/RemoteFileName"/>
                        </RemoteFileName>
                        <Data>
                            <xsl:value-of select="$Start/FTP/Put/Data"/>
                        </Data>
                        <Host>
                            <xsl:value-of select="$Start/FTP/Host"/>
                        </Host>
                        <Port>
                            <xsl:value-of select="$Start/FTP/Port"/>
                        </Port>
                        <UserName>
                            <xsl:value-of select="$Start/FTP/Username"/>
                        </UserName>
                        <Password>
                            <xsl:value-of select="$Start/FTP/Password"/>
                        </Password>
                    </FTPPutInputDataASCII>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="FTP Rename">
                <pd:type>com.tibco.plugin.ftp.FTPRenameActivity</pd:type>
                <pd:resourceType>ae.activities.FTPRenameActivityUI</pd:resourceType>
                <pd:x>201</pd:x>
                <pd:y>416</pd:y>
                <config/>
                <pd:inputBindings>
                    <Input>
                        <Host>
                            <xsl:value-of select="$Start/FTP/Host"/>
                        </Host>
                        <Port>
                            <xsl:value-of select="$Start/FTP/Port"/>
                        </Port>
                        <UserName>
                            <xsl:value-of select="$Start/FTP/Username"/>
                        </UserName>
                        <Password>
                            <xsl:value-of select="$Start/FTP/Password"/>
                        </Password>
                        <xsl:if test="$Start/FTP/Rename/OldRemoteDirectory">
                            <OldRemoteDirectory>
                                <xsl:value-of select="$Start/FTP/Rename/OldRemoteDirectory"/>
                            </OldRemoteDirectory>
                        </xsl:if>
                        <OldRemoteFileName>
                            <xsl:value-of select="$Start/FTP/Rename/OldRemoteFileName"/>
                        </OldRemoteFileName>
                        <xsl:if test="$Start/FTP/Rename/NewRemoteDirectory">
                            <NewRemoteDirectory>
                                <xsl:value-of select="$Start/FTP/Rename/NewRemoteDirectory"/>
                            </NewRemoteDirectory>
                        </xsl:if>
                        <NewRemoteFileName>
                            <xsl:value-of select="$Start/FTP/Rename/NewRemoteFileName"/>
                        </NewRemoteFileName>
                    </Input>
                </pd:inputBindings>
            </pd:activity>
        </pd:group>
        <pd:activity name="GEH - FTP Error">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>449</pd:x>
            <pd:y>418</pd:y>
            <config>
                <processName>/Common/Functions/GEH.process</processName>
            </config>
            <pd:inputBindings>
                <GEH>
                    <xsl:copy-of select="$_error/ns2:ErrorReport"/>
                    <ns3:Context>
                        <ns3:ProcessInput>
                            <xsl:copy-of select="$Start/FTP"/>
                        </ns3:ProcessInput>
                    </ns3:Context>
                    <Retry>
                        <RetryCount>
                            <xsl:value-of select="$retryCount"/>
                        </RetryCount>
                        <xsl:copy-of select="$Start/FTP/ns22:RetryOptions"/>
                    </Retry>
                </GEH>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Give Up (TechError)">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>642</pd:x>
        <pd:y>178</pd:y>
        <config>
            <faultName>TechError</faultName>
        </config>
        <pd:inputBindings>
            <ns12:ActivityInput>
                <message/>
                <data>
                    <ns22:TechError>
                        <ns22:DescShort>
                            <xsl:value-of select="$_error/ns2:ErrorReport/Msg"/>
                        </ns22:DescShort>
                        <ns22:ErrorReport>
                            <xsl:copy-of select="$_error/ns2:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                            <xsl:copy-of select="$_error/ns2:ErrorReport/@*"/>
                            <xsl:copy-of select="$_error/ns2:ErrorReport/node()"/>
                        </ns22:ErrorReport>
                        <ns22:Retry>
                            <ns22:RetryCount>
                                <xsl:value-of select="$retryCount"/>
                            </ns22:RetryCount>
                            <xsl:copy-of select="$Start/FTP/ns22:RetryOptions"/>
                        </ns22:Retry>
                    </ns22:TechError>
                </data>
            </ns12:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log - Finished">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>642</pd:x>
        <pd:y>318</pd:y>
        <config>
            <role>%%Constants/Logging/Roles/Debug%%</role>
        </config>
        <pd:inputBindings>
            <ns1:ActivityInput>
                <message>
                    <xsl:value-of select="''"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Constants/Logging/Step/End"/>
                </msgCode>
            </ns1:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log - Start">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>412</pd:x>
        <pd:y>35</pd:y>
        <config>
            <role>%%Constants/Logging/Roles/Debug%%</role>
        </config>
        <pd:inputBindings>
            <ns1:ActivityInput>
                <message>
                    <xsl:value-of select="tib:render-xml($Start/FTP, 1,0)"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Constants/Logging/Step/Start"/>
                </msgCode>
            </ns1:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Log - Start</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log - Finished</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log - Start</pd:from>
        <pd:to>FTP Operations</pd:to>
        <pd:xpathDescription/>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>FTP Operations</pd:from>
        <pd:to>Give Up (TechError)</pd:to>
        <pd:xpathDescription>Give Up</pd:xpathDescription>
        <pd:lineType>Curved</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$GEH---FTP-Error/GEH/GiveUp = 1</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>FTP Operations</pd:from>
        <pd:to>Log - Finished</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:label>
        <pd:name>Label</pd:name>
        <pd:description>Allows FTP operations to be performed with the
specified retry strategy.</pd:description>
        <pd:x>135</pd:x>
        <pd:y>28</pd:y>
        <pd:width>268</pd:width>
        <pd:height>55</pd:height>
        <pd:fontColor>-16777216</pd:fontColor>
        <pd:color>-1</pd:color>
        <pd:type>solid</pd:type>
        <pd:thickness>0</pd:thickness>
        <pd:fade>true</pd:fade>
    </pd:label>
</pd:ProcessDefinition>