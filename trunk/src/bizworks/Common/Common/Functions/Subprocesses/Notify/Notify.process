<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx2="http://code.google.com/tvillars/tibco/bw/Common/Schemas/Common" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns="http://code.google.com/tvillars/tibco/bw/Common/Functions/MailSend" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns11="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:pfx21="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://code.google.com/tvillars/tibco/bw/TechServices/EMail" xmlns:pfx3="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH">
    <xsd:import namespace="http://code.google.com/tvillars/tibco/bw/Common/Schemas/Common" schemaLocation="/Common/Schemas/Common.xsd"/>
    <xsd:import namespace="http://www.tibco.com/pe/EngineTypes" schemaLocation="/Common/Schemas/BizWorks/EngineTypes.xsd"/>
    <xsd:import namespace="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" schemaLocation="/Common/Functions/GEH.xsd"/>
    <pd:name>Common/Functions/Subprocesses/Notify/Notify.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Notify">
            <xsd:complexType>
                <xsd:choice>
                    <xsd:element ref="pfx3:DataInfo"/>
                    <xsd:element ref="pfx3:DataWarn"/>
                    <xsd:element ref="pfx3:TechInfo"/>
                    <xsd:element ref="pfx3:TechWarn"/>
                    <xsd:element ref="pfx3:DataError"/>
                    <xsd:element ref="pfx3:TechError"/>
                    <xsd:element ref="pfx3:Error"/>
                </xsd:choice>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>91</pd:startX>
    <pd:startY>199</pd:startY>
    <pd:returnBindings>
        <Notify>
            <message>
                <xsl:value-of select="tib:render-xml($Start/Notify/*, 1,1)"/>
            </message>
            <xsl:if test="($Start/Notify//MsgCode)[last()]">
                <msgCode>
                    <xsl:value-of select="($Start/Notify//MsgCode)[last()]"/>
                </msgCode>
            </xsl:if>
            <NotifyInput>
                <xsl:copy-of select="$Start/Notify/ancestor-or-self::*/namespace::node()"/>
                <xsl:copy-of select="$Start/Notify/@*"/>
                <xsl:copy-of select="$Start/Notify/node()"/>
            </NotifyInput>
        </Notify>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="Notify">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="message" type="xsd:string"/>
                    <xsd:element name="msgCode" type="xsd:string" minOccurs="0"/>
                    <xsd:element name="NotifyInput">
                        <xsd:complexType>
                            <xsd:choice>
                                <xsd:element ref="pfx3:DataInfo"/>
                                <xsd:element ref="pfx3:DataWarn"/>
                                <xsd:element ref="pfx3:TechInfo"/>
                                <xsd:element ref="pfx3:TechWarn"/>
                                <xsd:element ref="pfx3:DataError"/>
                                <xsd:element ref="pfx3:TechError"/>
                                <xsd:element ref="pfx3:Error"/>
                            </xsd:choice>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>597</pd:endX>
    <pd:endY>89</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:activity name="EMail">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>597</pd:x>
        <pd:y>199</pd:y>
        <config>
            <processName>/Services/Technical/EMail/Public/EMail.process</processName>
        </config>
        <pd:inputBindings>
            <ns3:EMail>
                <from>
                    <xsl:value-of select="concat(&#xA;$_globalVariables/ns1:GlobalVariables/Environment&#xA;,'-'&#xA;,$_processContext/pfx21:ProcessContext/EngineName&#xA;,'@chicos.com')"/>
                </from>
                <xsl:choose>
                    <!--Data Issue-->
                    <xsl:when test="exists($Start/Notify/pfx3:DataInfo)&#xA;or&#xA;exists($Start/Notify/pfx3:DataWarn)">
                        <xsl:for-each select="$Get-Email-Recipient-List/root/Data">
                            <to>
                                <xsl:value-of select="."/>
                            </to>
                        </xsl:for-each>
                    </xsl:when>
                    <!--Technical Issue-->
                    <xsl:when test="exists($Start/Notify/pfx3:TechInfo)&#xA;or&#xA;exists($Start/Notify/pfx3:TechWarn)">
                        <xsl:for-each select="$Get-Email-Recipient-List/root/Tech">
                            <to>
                                <xsl:value-of select="."/>
                            </to>
                        </xsl:for-each>
                    </xsl:when>
                    <!--Unknown Issue (should never go here)-->
                    <xsl:otherwise>
                        <xsl:for-each select="$Get-Email-Recipient-List/root/Tibco">
                            <to>
                                <xsl:value-of select="."/>
                            </to>
                        </xsl:for-each>
                    </xsl:otherwise>
                </xsl:choose>
                <subject>
                    <xsl:value-of select="concat(&#xA;local-name($Start/Notify/*[1])&#xA;,' - '&#xA;,$Start/Notify/*[1]/pfx3:DescShort)"/>
                </subject>
                <!--add location of log file-->
                <bodyText>
                    <xsl:value-of select="concat(&#xA;&quot;&#xD;&#xA;&quot;&#xA;,&quot;&#xD;&#xA;&quot;&#xA;,'Log File: '&#xA;,'\\',$ServerInfo/ServerInfo/HostName, '\', $_globalVariables/ns1:GlobalVariables/DirTrace,'\', $_processContext/pfx21:ProcessContext/EngineName,'.log')"/>
                </bodyText>
                <xsl:for-each select="$Start/Notify//pfx3:DescLong">
                    <bodyText>
                        <xsl:value-of select="."/>
                    </bodyText>
                </xsl:for-each>
                <!--Seperate from main error text with a blank line-->
                <xsl:if test="count($Map-ErrorReport-To-Human-Readable-Description/MapErrorReportToDesc/ErrorReportDesc)>0">
                    <bodyText>
                        <xsl:value-of select="''"/>
                    </bodyText>
                    <bodyText>
                        <xsl:value-of select="'Biz Works ErrorReport'"/>
                    </bodyText>
                    <bodyText>
                        <xsl:value-of select="'---------------------'"/>
                    </bodyText>
                    <xsl:for-each select="$Map-ErrorReport-To-Human-Readable-Description/MapErrorReportToDesc/ErrorReportDesc">
                        <bodyText>
                            <xsl:value-of select="."/>
                        </bodyText>
                    </xsl:for-each>
                </xsl:if>
                <!--ErrorReport-->
                <xsl:if test="exists($Start/Notify//pfx3:ErrorReport)">
                    <XMLAttachment>
                        <xsl:attribute name="description">
                            <xsl:value-of select="'ErrorReport'"/>
                        </xsl:attribute>
                        <xsl:copy-of select="$Start/Notify/pfx3:DataWarn/pfx3:ErrorReport&#xA;| $Start/Notify/pfx3:TechWarn/pfx3:ErrorReport&#xA;| $Start/Notify/pfx3:DataError/pfx3:ErrorReport&#xA;| $Start/Notify/pfx3:TechError/pfx3:ErrorReport&#xA;| $Start/Notify/pfx3:Error/pfx3:ErrorReport"/>
                    </XMLAttachment>
                </xsl:if>
                <!--Turn all Context info into XML attachments-->
                <xsl:for-each select="$Map-Context-Into-XML-Attachments/XMLAttachments/pfx2:UntypedData">
                    <XMLAttachment>
                        <xsl:if test="@description">
                            <xsl:attribute name="description">
                                <xsl:value-of select="@description"/>
                            </xsl:attribute>
                        </xsl:if>
                        <xsl:copy-of select="*"/>
                    </XMLAttachment>
                </xsl:for-each>
                <!--Global Vars-->
                <xsl:if test="exists($Get-GlobalVars-Minus-Passwords/GetGlobalVarsMinusPasswords)">
                    <XMLAttachment>
                        <xsl:attribute name="description">
                            <xsl:value-of select="'GlobalVars'"/>
                        </xsl:attribute>
                        <xsl:copy-of select="$Get-GlobalVars-Minus-Passwords/GetGlobalVarsMinusPasswords"/>
                    </XMLAttachment>
                </xsl:if>
            </ns3:EMail>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Get Email Recipient List">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>267</pd:x>
        <pd:y>124</pd:y>
        <config>
            <processName>/Common/Functions/Subprocesses/Notify/GetEMailRecipientList.process</processName>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Get GlobalVars Minus Passwords">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>94</pd:x>
        <pd:y>492</pd:y>
        <config>
            <processName>/Common/Functions/Subprocesses/Notify/GetGlobalVarsMinusPasswords.process</processName>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Map Context Into XML Attachments">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>267</pd:x>
        <pd:y>312</pd:y>
        <config>
            <element>
                <xsd:element name="XMLAttachments">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="pfx2:UntypedData" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <XMLAttachments>
                <xsl:for-each select="$Start/Notify/*/*/pfx2:ProcessInput">
                    <pfx2:UntypedData>
                        <xsl:if test="@description">
                            <xsl:attribute name="description">
                                <xsl:value-of select="@description"/>
                            </xsl:attribute>
                        </xsl:if>
                        <xsl:copy-of select="*"/>
                    </pfx2:UntypedData>
                </xsl:for-each>
                <xsl:for-each select="$Start/Notify/*/*/pfx2:ActivityInput">
                    <pfx2:UntypedData>
                        <xsl:if test="@description">
                            <xsl:attribute name="description">
                                <xsl:value-of select="@description"/>
                            </xsl:attribute>
                        </xsl:if>
                        <xsl:copy-of select="*"/>
                    </pfx2:UntypedData>
                </xsl:for-each>
                <xsl:for-each select="$Start/Notify/*/*/pfx2:ActivityOutputs">
                    <pfx2:UntypedData>
                        <xsl:if test="@description">
                            <xsl:attribute name="description">
                                <xsl:value-of select="@description"/>
                            </xsl:attribute>
                        </xsl:if>
                        <xsl:copy-of select="*"/>
                    </pfx2:UntypedData>
                </xsl:for-each>
                <xsl:for-each select="$Start/Notify/*/*/pfx2:ExternalSystem">
                    <pfx2:UntypedData>
                        <xsl:attribute name="description">
                            <xsl:value-of select="'ExternalSystem'"/>
                        </xsl:attribute>
                        <xsl:copy-of select="*"/>
                    </pfx2:UntypedData>
                </xsl:for-each>
            </XMLAttachments>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map ErrorReport To Human Readable Description">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>368</pd:x>
        <pd:y>214</pd:y>
        <config>
            <processName>/Common/Functions/Subprocesses/Notify/MapErrorReportToDesc.process</processName>
        </config>
        <pd:inputBindings>
            <MapErrorReportToDesc>
                <xsl:choose>
                    <xsl:when test="exists($Start/Notify/pfx3:DataWarn)">
                        <pfx21:ErrorReport>
                            <xsl:copy-of select="$Start/Notify/pfx3:DataWarn/pfx3:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                            <xsl:copy-of select="$Start/Notify/pfx3:DataWarn/pfx3:ErrorReport/@*"/>
                            <xsl:copy-of select="$Start/Notify/pfx3:DataWarn/pfx3:ErrorReport/node()"/>
                        </pfx21:ErrorReport>
                    </xsl:when>
                    <xsl:when test="exists($Start/Notify/pfx3:TechWarn)">
                        <pfx21:ErrorReport>
                            <xsl:copy-of select="$Start/Notify/pfx3:TechWarn/pfx3:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                            <xsl:copy-of select="$Start/Notify/pfx3:TechWarn/pfx3:ErrorReport/@*"/>
                            <xsl:copy-of select="$Start/Notify/pfx3:TechWarn/pfx3:ErrorReport/node()"/>
                        </pfx21:ErrorReport>
                    </xsl:when>
                    <xsl:when test="exists($Start/Notify/pfx3:DataError)">
                        <pfx21:ErrorReport>
                            <xsl:copy-of select="$Start/Notify/pfx3:DataError/pfx3:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                            <xsl:copy-of select="$Start/Notify/pfx3:DataError/pfx3:ErrorReport/@*"/>
                            <xsl:copy-of select="$Start/Notify/pfx3:DataError/pfx3:ErrorReport/node()"/>
                        </pfx21:ErrorReport>
                    </xsl:when>
                    <xsl:when test="exists($Start/Notify/pfx3:TechError)">
                        <pfx21:ErrorReport>
                            <xsl:copy-of select="$Start/Notify/pfx3:TechError/pfx3:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                            <xsl:copy-of select="$Start/Notify/pfx3:TechError/pfx3:ErrorReport/@*"/>
                            <xsl:copy-of select="$Start/Notify/pfx3:TechError/pfx3:ErrorReport/node()"/>
                        </pfx21:ErrorReport>
                    </xsl:when>
                    <xsl:when test="exists($Start/Notify/pfx3:Error)">
                        <pfx21:ErrorReport>
                            <xsl:copy-of select="$Start/Notify/pfx3:Error/pfx3:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                            <xsl:copy-of select="$Start/Notify/pfx3:Error/pfx3:ErrorReport/@*"/>
                            <xsl:copy-of select="$Start/Notify/pfx3:Error/pfx3:ErrorReport/node()"/>
                        </pfx21:ErrorReport>
                    </xsl:when>
                </xsl:choose>
            </MapErrorReportToDesc>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ServerInfo">
