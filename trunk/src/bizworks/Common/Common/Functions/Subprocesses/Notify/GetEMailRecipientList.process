<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns1="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/mail" xmlns:pfx="http://xmlns.tibco.com/body" xmlns:ns3="http://www.tibco.com/pe/EngineTypes" xmlns:ns4="www.tibco.com/plugin/Sleep" xmlns:ns42="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema">
    <pd:name>Common/Functions/Subprocesses/Notify/GetEMailRecipientList.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>50</pd:startX>
    <pd:startY>316</pd:startY>
    <pd:returnBindings>
        <root>
            <Data>
                <xsl:value-of select="'tom.villars@bp.com'"/>
            </Data>
            <Tech>
                <xsl:value-of select="'tom.villars@bp.com'"/>
            </Tech>
            <Tibco>
                <xsl:value-of select="'tom.villars@bp.com'"/>
            </Tibco>
        </root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="root">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Data" type="xsd:string" maxOccurs="unbounded"/>
                    <xsd:element name="Tech" type="xsd:string" maxOccurs="unbounded"/>
                    <xsd:element name="Tibco" type="xsd:string" maxOccurs="unbounded"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>386</pd:endX>
    <pd:endY>440</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:group name="Get Recepient List">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>124</pd:x>
        <pd:y>1</pd:y>
        <pd:width>521</pd:width>
        <pd:height>376</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <serializable>false</serializable>
            <pd:indexSlot>retryCount</pd:indexSlot>
            <pd:errorCondition>false()</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Null-2</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Null-2</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Log - TechError</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Generate Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Log - TechWarning</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Null</pd:from>
            <pd:to>Log - TechError</pd:to>
            <pd:xpathDescription>error</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>($retryCount &gt;= $_globalVariables/ns:GlobalVariables/Common/Functions/GEH/BeforeFirstError/retryCountToLogError)
and (
  ($retryCount = $_globalVariables/ns:GlobalVariables/Common/Functions/GEH/BeforeFirstError/retryCountToLogError)
  or
  ($retryCount mod $_globalVariables/ns:GlobalVariables/Common/Functions/GEH/SubsequentError/retryCountToLogError =0)
)</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Null</pd:from>
            <pd:to>Log - TechWarning</pd:to>
            <pd:xpathDescription>warn</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Null-2</pd:from>
            <pd:to>Null</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65536</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:activity name="Log - TechError">
            <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
            <pd:resourceType>ae.activities.log</pd:resourceType>
            <pd:x>332</pd:x>
            <pd:y>197</pd:y>
            <config>
                <role>%%Constants/Logging/Roles/TechError%%</role>
            </config>
            <pd:inputBindings>
                <ns1:ActivityInput>
                    <message>
                        <xsl:value-of select="concat(&#xA;'EngineName[',$Engine-Name/EngineName,']'&#xA;,'  ',tib:render-xml($_error/ns3:ErrorReport))"/>
                    </message>
                    <xsl:if test="$_error/ns3:ErrorReport/MsgCode">
                        <msgCode>
                            <xsl:value-of select="$_error/ns3:ErrorReport/MsgCode"/>
                        </msgCode>
                    </xsl:if>
                </ns1:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>332</pd:x>
            <pd:y>309</pd:y>
            <config/>
            <pd:inputBindings>
                <ns4:SleepInputSchema>
                    <xsl:choose>
                        <xsl:when test="$retryCount &lt;= $_globalVariables/ns:GlobalVariables/Common/Functions/GEH/BeforeFirstError/retryCountToLogError">
                            <IntervalInMillisec>
                                <xsl:value-of select="1000 * $_globalVariables/ns:GlobalVariables/Common/Functions/GEH/BeforeFirstError/SecsToPauseBetweenRetries"/>
                            </IntervalInMillisec>
                        </xsl:when>
                        <xsl:otherwise>
                            <IntervalInMillisec>
                                <xsl:value-of select="1000 * $_globalVariables/ns:GlobalVariables/Common/Functions/GEH/SubsequentError/SecsToPauseBetweenRetries"/>
                            </IntervalInMillisec>
                        </xsl:otherwise>
                    </xsl:choose>
                </ns4:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Generate Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>454</pd:x>
            <pd:y>309</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns42:ActivityInput>
                    <message>
                        <xsl:value-of select="$_error/ns3:ErrorReport/Msg"/>
                    </message>
                    <xsl:if test="$_error/ns3:ErrorReport/MsgCode">
                        <messageCode>
                            <xsl:value-of select="$_error/ns3:ErrorReport/MsgCode"/>
                        </messageCode>
                    </xsl:if>
                </ns42:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Log - TechWarning">
            <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
            <pd:resourceType>ae.activities.log</pd:resourceType>
            <pd:x>236</pd:x>
            <pd:y>310</pd:y>
            <config>
                <role>%%Constants/Logging/Roles/TechWarning%%</role>
            </config>
            <pd:inputBindings>
                <ns1:ActivityInput>
                    <message>
                        <xsl:value-of select="concat(&#xA;'EngineName[',$Engine-Name/EngineName,']'&#xA;,'  ',tib:render-xml($_error/ns3:ErrorReport))"/>
                    </message>
                    <xsl:if test="$_error/ns3:ErrorReport/MsgCode">
                        <msgCode>
                            <xsl:value-of select="$_error/ns3:ErrorReport/MsgCode"/>
                        </msgCode>
                    </xsl:if>
                </ns1:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Null">
            <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
            <pd:resourceType>ae.activities.null</pd:resourceType>
            <pd:x>236</pd:x>
            <pd:y>197</pd:y>
            <config/>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="Null-2">
            <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
            <pd:resourceType>ae.activities.null</pd:resourceType>
            <pd:x>240</pd:x>
            <pd:y>70</pd:y>
            <config/>
            <pd:inputBindings/>
        </pd:activity>
    </pd:group>
    <pd:activity name="Engine Name">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>49</pd:x>
        <pd:y>197</pd:y>
        <config>
            <element>
                <xsd:element name="EngineName" type="xsd:string"/>
            </element>
        </config>
        <pd:inputBindings>
            <xsl:choose>
                <!--There is no suffix at the end of the engine name-->
                <xsl:when test="count(tib:tokenize($_processContext/ns3:ProcessContext/EngineName, '-'))=2">
                    <EngineName>
                        <xsl:value-of select="$_processContext/ns3:ProcessContext/EngineName"/>
                    </EngineName>
                </xsl:when>
                <!--There is a suffix, such as -P1 or -S, and the end of the engine name-->
                <xsl:when test="count(tib:tokenize($_processContext/ns3:ProcessContext/EngineName, '-'))=3">
                    <EngineName>
                        <xsl:value-of select="tib:substring-before-last($_processContext/ns3:ProcessContext/EngineName, '-')"/>
                    </EngineName>
                </xsl:when>
                <!--Goes here when testing in debug mode in the Designer-->
                <!--Should only go here when running in the debugger-->
                <xsl:otherwise>
                    <EngineName>
                        <xsl:value-of select="$_processContext/ns3:ProcessContext/EngineName"/>
                    </EngineName>
                </xsl:otherwise>
            </xsl:choose>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Null-1">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>138</pd:x>
        <pd:y>438</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Engine Name</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>false()</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Get Recepient List</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Engine Name</pd:from>
        <pd:to>Get Recepient List</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Null-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null-1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:label>
        <pd:name>Label (1)</pd:name>
        <pd:description>Since this process is a dependy for the
GEH, we'll get an infinite recursive loop
if  we use the GEH process to handle an
issue with the database call.</pd:description>
        <pd:x>517</pd:x>
        <pd:y>246</pd:y>
        <pd:width>251</pd:width>
        <pd:height>83</pd:height>
        <pd:fontColor>-16777216</pd:fontColor>
        <pd:color>-1</pd:color>
        <pd:type>solid</pd:type>
        <pd:thickness>0</pd:thickness>
        <pd:fade>true</pd:fade>
    </pd:label>
</pd:ProcessDefinition>