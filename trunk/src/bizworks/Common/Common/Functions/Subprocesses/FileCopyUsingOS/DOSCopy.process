<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx="http://www.tibco.com/ns/no_namespace_schema_location/Schemas/tibco/xsd/bw/Cmd.xsd" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:ns0="http://code.google.com/tvillars/tibco/bw/Common/Schemas/Common" xmlns:ns02="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns2="http://www.tibco.com/pe/DeployedVarsType">
    <xsd:import namespace="http://www.tibco.com/ns/no_namespace_schema_location/Schemas/tibco/xsd/bw/Cmd.xsd" schemaLocation="/Common/Schemas/BizWorks/Cmd.xsd"/>
    <xsd:import namespace="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" schemaLocation="/Common/Functions/GEH.xsd"/>
    <pd:name>Common/Functions/Subprocesses/FileCopyUsingOS/DOSCopy.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="DOSCopy">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="source" type="xsd:string"/>
                    <xsd:element name="target" type="xsd:string"/>
                    <xsd:element name="workingDirectory" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>55</pd:startX>
    <pd:startY>157</pd:startY>
    <pd:returnBindings>
        <xsl:copy-of select="$DOS-Copy/CmdOutputWithActivityOutput"/>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx:CmdOutputWithActivityOutput"/>
    <pd:endX>622</pd:endX>
    <pd:endY>74</pd:endY>
    <pd:errorSchemas>
        <TechError ref="ns02:TechError"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:group name="DOS Copy, if error then log, pause and retry">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>112</pd:x>
        <pd:y>11</pd:y>
        <pd:width>339</pd:width>
        <pd:height>276</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <serializable>false</serializable>
            <pd:indexSlot>retryCount</pd:indexSlot>
            <pd:errorCondition>false()</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>DOS Copy</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>DOS Copy</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>DOS Copy</pd:from>
            <pd:to>GEH</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65536</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:activity name="DOS Copy">
            <pd:type>com.tibco.plugin.cmdexec.CmdExecActivity</pd:type>
            <pd:resourceType>ae.activities.CmdExecActivity</pd:resourceType>
            <pd:x>262</pd:x>
            <pd:y>73</pd:y>
            <config>
                <outputToString>true</outputToString>
                <lineSplit>None</lineSplit>
            </config>
            <pd:inputBindings>
                <CmdInput>
                    <command>
                        <xsl:value-of select="concat('cmd /c copy &quot;', $Start/DOSCopy/source,'&quot; &quot;',$Start/DOSCopy/target,'&quot;')"/>
                    </command>
                    <xsl:if test="$Start/DOSCopy/workingDirectory">
                        <workingDirectory>
                            <xsl:value-of select="$Start/DOSCopy/workingDirectory"/>
                        </workingDirectory>
                    </xsl:if>
                </CmdInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="GEH">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>264</pd:x>
            <pd:y>193</pd:y>
            <config>
                <processName>/Common/Functions/GEH.process</processName>
            </config>
            <pd:inputBindings>
                <GEH>
                    <xsl:copy-of select="$_error/ns1:ErrorReport"/>
                    <ns0:Context>
                        <ns0:ProcessInput>
                            <xsl:copy-of select="$Start/DOSCopy"/>
                        </ns0:ProcessInput>
                    </ns0:Context>
                    <Retry>
                        <RetryCount>
                            <xsl:value-of select="$retryCount"/>
                        </RetryCount>
                        <ns02:RetryOptions>
                            <ns02:TryForever/>
                        </ns02:RetryOptions>
                    </Retry>
                </GEH>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="TechError (DOS Copy)">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>611</pd:x>
        <pd:y>255</pd:y>
        <config>
            <faultName>TechError</faultName>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <xsl:if test="count($DOS-Copy/CmdOutputWithActivityOutput/error)>0">
                    <message>
                        <xsl:value-of select="$DOS-Copy/CmdOutputWithActivityOutput/error[1]"/>
                    </message>
                </xsl:if>
                <messageCode>
                    <xsl:value-of select="concat('DOSCopy-',$DOS-Copy/CmdOutputWithActivityOutput/returnCode)"/>
                </messageCode>
                <data>
                    <ns02:TechError>
                        <ns0:Context>
                            <ns0:ProcessInput>
                                <xsl:copy-of select="$Start/DOSCopy"/>
                            </ns0:ProcessInput>
                            <ns0:ActivityOutputs>
                                <xsl:copy-of select="$DOS-Copy/CmdOutputWithActivityOutput"/>
                            </ns0:ActivityOutputs>
                            <ns0:ExternalSystem/>
                        </ns0:Context>
                        <ns02:DescShort>
                            <xsl:value-of select="concat(&#xA;$Start/DOSCopy/workingDirectory&#xA;,if (exists($Start/DOSCopy/workingDirectory)) then ' ' else ''&#xA;,'Copy &quot;',$Start/DOSCopy/source, '&quot;  &quot;', $Start/DOSCopy/target,'&quot;'&#xA;,' - '&#xA;,$DOS-Copy/CmdOutputWithActivityOutput/error[1])"/>
                        </ns02:DescShort>
                        <ns02:DescLong>
                            <xsl:value-of select="'Unable to perform the following DOS Copy Command:'"/>
                        </ns02:DescLong>
                        <ns02:DescLong>
                            <xsl:value-of select="''"/>
                        </ns02:DescLong>
                        <ns02:DescLong>
                            <xsl:value-of select="concat('cmd /c ',  $Start/DOSCopy/workingDirectory, ' copy &quot;', $Start/DOSCopy/source,'&quot; &quot;',$Start/DOSCopy/target,'&quot;')"/>
                        </ns02:DescLong>
                        <ns02:DescLong>
                            <xsl:value-of select="''"/>
                        </ns02:DescLong>
                        <ns02:DescLong>
                            <xsl:value-of select="concat('Return Code: ',$DOS-Copy/CmdOutputWithActivityOutput/returnCode)"/>
                        </ns02:DescLong>
                        <ns02:DescLong>
                            <xsl:value-of select="tib:concat-sequence-format($DOS-Copy/CmdOutputWithActivityOutput/output,&quot;&#xD;&#xA;&quot;)"/>
                        </ns02:DescLong>
                        <ns02:DescLong>
                            <xsl:value-of select="tib:concat-sequence-format($DOS-Copy/CmdOutputWithActivityOutput/error,&quot;&#xD;&#xA;&quot;)"/>
                        </ns02:DescLong>
                    </ns02:TechError>
                </data>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>DOS Copy, if error then log, pause and retry</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>DOS Copy, if error then log, pause and retry</pd:from>
        <pd:to>End</pd:to>
        <pd:xpathDescription>sucess</pd:xpathDescription>
        <pd:lineType>Curved</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$DOS-Copy/CmdOutputWithActivityOutput/returnCode = 0</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>DOS Copy, if error then log, pause and retry</pd:from>
        <pd:to>TechError (DOS Copy)</pd:to>
        <pd:lineType>Curved</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>