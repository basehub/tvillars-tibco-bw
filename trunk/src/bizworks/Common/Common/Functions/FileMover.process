<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns="http://www.tibco.com/pe/DeployedVarsType" xmlns:ns1="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://www.tibco.com/pe/EngineTypes" xmlns:ns21="http://xmlns.tibco.com/body" xmlns:ns22="http://xmlns.tibco.com/encodings/mime" xmlns:ns11="http://www.tibco.com/namespaces/tnt/plugins/mail" xmlns:ns3="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pfx2="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" xmlns:ns4="http://code.google.com/tvillars/tibco/bw/Common/Schemas/Common" xmlns:ns5="http://schemas.tibco.com/bw/plugins/file/5.0/fileExceptions">
    <xsd:import namespace="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" schemaLocation="/Common/Functions/GEH.xsd"/>
    <pd:name>Common/Functions/FileMover.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="FileMover">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="FilesToMove" maxOccurs="unbounded">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="fromFileName" type="xsd:string"/>
                                <xsd:element name="toFileName" type="xsd:string"/>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                    <xsd:element ref="pfx2:RetryOptions"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>307</pd:startX>
    <pd:startY>68</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>552</pd:endX>
    <pd:endY>128</pd:endY>
    <pd:errorSchemas>
        <TechError ref="pfx2:TechError"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:group name="Loop throw each file and move it">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>50</pd:x>
        <pd:y>129</pd:y>
        <pd:width>374</pd:width>
        <pd:height>304</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>repeat</pd:groupType>
            <serializable>false</serializable>
            <pd:indexSlot>fileIndex</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:repeatCondition>($fileIndex = count($Start/FileMover/FilesToMove))
or
($GEH/GEH/GiveUp = "true")</pd:repeatCondition>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>if Error then log, pause and retry as specified</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>if Error then log, pause and retry as specified</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:group name="if Error then log, pause and retry as specified">
            <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
            <pd:resourceType>ae.process.group</pd:resourceType>
            <pd:x>100</pd:x>
            <pd:y>166</pd:y>
            <pd:width>268</pd:width>
            <pd:height>230</pd:height>
            <pd:collapsedWidth>57</pd:collapsedWidth>
            <pd:collapsedHeight>70</pd:collapsedHeight>
            <pd:joinToggle>inherit</pd:joinToggle>
            <config>
                <pd:groupType>errorLoop</pd:groupType>
                <serializable>false</serializable>
                <pd:indexSlot>retryCount</pd:indexSlot>
                <pd:errorCondition>$GEH/GEH/GiveUp = "true"</pd:errorCondition>
                <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
            </config>
            <pd:inputBindings/>
            <pd:expanded>true</pd:expanded>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>Rename File</pd:to>
                <pd:lineType>Multiple Bends</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>GEH</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>Rename File</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>Rename File</pd:from>
                <pd:to>GEH</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-65536</pd:lineColor>
                <pd:conditionType>error</pd:conditionType>
            </pd:transition>
            <pd:activity name="Rename File">
                <pd:type>com.tibco.plugin.file.FileRenameActivity</pd:type>
                <pd:resourceType>ae.activities.FileRenameActivity</pd:resourceType>
                <pd:x>231</pd:x>
                <pd:y>221</pd:y>
                <config>
                    <overwrite>true</overwrite>
                    <createMissingDirectories>true</createMissingDirectories>
                </config>
                <pd:inputBindings>
                    <ns0:RenameActivityConfig>
                        <fromFileName>
                            <xsl:value-of select="$Start/FileMover/FilesToMove[$fileIndex]/fromFileName"/>
                        </fromFileName>
                        <toFileName>
                            <xsl:value-of select="$Start/FileMover/FilesToMove[$fileIndex]/toFileName"/>
                        </toFileName>
                    </ns0:RenameActivityConfig>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="GEH">
                <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
                <pd:resourceType>ae.process.subprocess</pd:resourceType>
                <pd:x>232</pd:x>
                <pd:y>320</pd:y>
                <config>
                    <processName>/Common/Functions/GEH.process</processName>
                </config>
                <pd:inputBindings>
                    <GEH>
                        <xsl:copy-of select="$_error/ns2:ErrorReport"/>
                        <ns4:Context>
                            <ns4:ProcessInput>
                                <xsl:copy-of select="$Start/FileMover"/>
                            </ns4:ProcessInput>
                        </ns4:Context>
                        <Retry>
                            <RetryCount>
                                <xsl:value-of select="$retryCount"/>
                            </RetryCount>
                            <xsl:copy-of select="$Start/FileMover/pfx2:RetryOptions"/>
                        </Retry>
                    </GEH>
                </pd:inputBindings>
            </pd:activity>
        </pd:group>
    </pd:group>
    <pd:activity name="Log - Finished">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>552</pd:x>
        <pd:y>239</pd:y>
        <config>
            <role>%%Constants/Logging/Roles/Debug%%</role>
        </config>
        <pd:inputBindings>
            <ns1:ActivityInput>
                <message>
                    <xsl:value-of select="tib:render-xml($Rename-File/ns0:RenameActivityOutput, 1,0)"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Constants/Logging/Step/Start"/>
                </msgCode>
            </ns1:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log - Start">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>404</pd:x>
        <pd:y>69</pd:y>
        <config>
            <role>%%Constants/Logging/Roles/Debug%%</role>
        </config>
        <pd:inputBindings>
            <ns1:ActivityInput>
                <message>
                    <xsl:value-of select="tib:render-xml($Start/FileMover, 1,0)"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/Constants/Logging/Step/Start"/>
                </msgCode>
            </ns1:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="TechError">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>552</pd:x>
        <pd:y>348</pd:y>
        <config>
            <faultName>TechError</faultName>
        </config>
        <pd:inputBindings>
            <ns3:ActivityInput>
                <data>
                    <pfx2:TechError>
                        <xsl:copy-of select="$GEH/GEH/GEHInput/ns4:Context"/>
                        <pfx2:DescShort>
                            <xsl:value-of select="concat(&#xA;'Moving File [', $Start/FileMover/FilesToMove[$fileIndex]/fromFileName&#xA;,'] to ['&#xA;,$Start/FileMover/FilesToMove[$fileIndex]/toFileName&#xA;,']'&#xA;,' - '&#xA;,tib:if-absent(local-name($_error_Rename-File/ActivityErrors/*), $_error/ns2:ErrorReport/Msg)&#xA;)"/>
                        </pfx2:DescShort>
                        <xsl:for-each select="$GEH/GEH/GEHInput/Retry">
                            <pfx2:Retry>
                                <pfx2:RetryCount>
                                    <xsl:value-of select="RetryCount"/>
                                </pfx2:RetryCount>
                                <xsl:copy-of select="pfx2:RetryOptions"/>
                            </pfx2:Retry>
                        </xsl:for-each>
                    </pfx2:TechError>
                </data>
            </ns3:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Loop throw each file and move it</pd:from>
        <pd:to>Log - Finished</pd:to>
        <pd:xpathDescription>sucess</pd:xpathDescription>
        <pd:lineType>Curved</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Loop throw each file and move it</pd:from>
        <pd:to>TechError</pd:to>
        <pd:xpathDescription>gave up</pd:xpathDescription>
        <pd:lineType>Curved</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$GEH/GEH/GiveUp = "true"</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Log - Start</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log - Finished</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log - Start</pd:from>
        <pd:to>Loop throw each file and move it</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:label>
        <pd:name>Label (1)</pd:name>
        <pd:description>Simple wrapper around the Rename File
activity to make it a multiple file mover.

Files will be overwritten

Destination directories will automatically
be created</pd:description>
        <pd:x>140</pd:x>
        <pd:y>63</pd:y>
        <pd:width>278</pd:width>
        <pd:height>125</pd:height>
        <pd:fontColor>-16777216</pd:fontColor>
        <pd:color>-1</pd:color>
        <pd:type>solid</pd:type>
        <pd:thickness>0</pd:thickness>
        <pd:fade>true</pd:fade>
    </pd:label>
</pd:ProcessDefinition>