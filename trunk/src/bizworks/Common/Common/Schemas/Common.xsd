<?xml version="1.0" encoding="UTF-8"?>

<schema xmlns="http://www.w3.org/2001/XMLSchema"
	 xmlns:chsCom="http://code.google.com/tvillars/tibco/bw/Common/Schemas/Common"
	 xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	 targetNamespace="http://code.google.com/tvillars/tibco/bw/Common/Schemas/Common"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<element name="uuId" type="chsCom:UUId"/>
	<simpleType name="UUId">
		<restriction base="xsd:string"/>
	</simpleType>
	<simpleType name="BizUnit">
		<restriction base="xsd:string">
			<enumeration value="Accounting"/>
			<enumeration value="BankingTax"/>
			<enumeration value="BizIntel"/>
			<enumeration value="HRPayroll"/>
		</restriction>
	</simpleType>
	<simpleType name="Environment">
		<restriction base="xsd:string">
			<enumeration value="dev-local"/>
			<enumeration value="dev"/>
			<enumeration value="test"/>
			<enumeration value="prod"/>
		</restriction>
	</simpleType>
	<simpleType name="TrxType_Type">
		<restriction base="xsd:string">
			<enumeration value="Change"/>
			<enumeration value="Create"/>
			<enumeration value="Delete"/>
			<enumeration value="Void"/>
		</restriction>
	</simpleType>
	<element name="bizUnit" type="chsCom:BizUnit"/>
	<element name="system" type="chsCom:System"/>
	<complexType name="System">
		<sequence>
			<element name="systemName" type="xsd:string"/>
			<element name="subSystemName" type="xsd:string" minOccurs="0"/>
			<element name="version" type="xsd:string"/>
		</sequence>
	</complexType>
	<element name="UntypedData" type="chsCom:UntypedData_Type"/>
	<complexType name="UntypedData_Type">
		<sequence>
			<any namespace="##any" processContents="skip" minOccurs="0"/>
		</sequence>
		<attribute name="description" type="xsd:string"/>
	</complexType>
	<element name="UntypedStuff" type="chsCom:UntypedStuff_Type"/>
	<complexType name="UntypedStuff_Type">
		<sequence>
			<any namespace="##any" processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
		<attribute name="description" type="xsd:string"/>
	</complexType>
	<element name="ProcessInput" type="chsCom:UntypedData_Type"/>
	<element name="ActivityInput" type="chsCom:UntypedData_Type"/>
	<element name="ActivityOutputs" type="chsCom:UntypedData_Type"/>
	<element name="ExternalSystem" type="chsCom:UntypedData_Type"/>
	<element name="Context" type="chsCom:Context_Type"/>
	<complexType name="Context_Type">
		<sequence>
			<element ref="chsCom:ProcessInput" minOccurs="0"/>
			<element ref="chsCom:ActivityInput" minOccurs="0"/>
			<element ref="chsCom:ActivityOutputs" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="chsCom:ExternalSystem" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
</schema>