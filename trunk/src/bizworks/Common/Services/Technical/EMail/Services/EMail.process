<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://www.chicos.com/eai/Common/Functions/MailSend" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:ns0="http://www.chicos.com/eai/Common/Schemas/Common" xmlns:ns02="http://www.chicos.com/eai/Common/Functions/GEH" xmlns:ns03="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:pfx2="http://www.chicos.com/eai/TechServices/EMail" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns12="http://www.tibco.com/pe/WriteToLogActivitySchema">
    <xsd:import namespace="http://www.chicos.com/eai/Common/Functions/MailSend" schemaLocation="/Common/Functions/MailSend.xsd"/>
    <xsd:import namespace="http://www.chicos.com/eai/TechServices/EMail" schemaLocation="/Services/Technical/EMail/Services/EMail.xsd"/>
    <pd:name>Services/Technical/EMail/Services/EMail.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx2:EMail"/>
    <pd:startX>456</pd:startX>
    <pd:startY>66</pd:startY>
    <pd:returnBindings>
        <EMail>
            <MessageID>
                <xsl:value-of select="$JMS-Queue-Sender/ns03:aEmptyOutputClass/ns03:MessageID"/>
            </MessageID>
        </EMail>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="EMail">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="MessageID" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>492</pd:endX>
    <pd:endY>280</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:group name="Publish EMail Service Request">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>38</pd:x>
        <pd:y>144</pd:y>
        <pd:width>289</pd:width>
        <pd:height>258</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <serializable>false</serializable>
            <pd:indexSlot>retryCount</pd:indexSlot>
            <pd:errorCondition>false()</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>JMS Queue Sender</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>JMS Queue Sender</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>JMS Queue Sender</pd:from>
            <pd:to>GEH</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65536</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:activity name="JMS Queue Sender">
            <pd:type>com.tibco.plugin.jms.JMSQueueSendActivity</pd:type>
            <pd:resourceType>ae.activities.JMSQueueSendActivity</pd:resourceType>
            <pd:x>161</pd:x>
            <pd:y>216</pd:y>
            <config>
                <PermittedMessageType>XML Text</PermittedMessageType>
                <SessionAttributes>
                    <transacted>false</transacted>
                    <acknowledgeMode>1</acknowledgeMode>
                    <maxSessions>1</maxSessions>
                    <destination>%%Services/Technical/EMail/Queue%%</destination>
                </SessionAttributes>
                <ConfigurableHeaders>
                    <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                    <JMSExpiration>%%Services/Technical/EMail/ExpirationSecs%%</JMSExpiration>
                    <JMSPriority>4</JMSPriority>
                    <JMSType/>
                </ConfigurableHeaders>
                <ConnectionReference>/Services/Technical/JMS/Channel.sharedjmscon</ConnectionReference>
                <InDataxsdString ref="pfx2:EMail"/>
            </config>
            <pd:inputBindings>
                <ns03:ActivityInput>
                    <Body>
                        <xsl:copy-of select="$Start/pfx2:EMail"/>
                    </Body>
                </ns03:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="GEH">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>164</pd:x>
            <pd:y>325</pd:y>
            <config>
                <processName>/Common/Functions/GEH.process</processName>
            </config>
            <pd:inputBindings>
                <GEH>
                    <xsl:copy-of select="$_error/ns:ErrorReport"/>
                    <ns0:Context>
                        <ns0:ProcessInput>
                            <xsl:copy-of select="$Start/pfx2:EMail"/>
                        </ns0:ProcessInput>
                        <ns0:ActivityOutputs/>
                    </ns0:Context>
                    <Retry>
                        <RetryCount>
                            <xsl:value-of select="$retryCount"/>
                        </RetryCount>
                        <ns02:RetryOptions>
                            <ns02:TryForever/>
                        </ns02:RetryOptions>
                    </Retry>
                </GEH>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Log - Finished">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>386</pd:x>
        <pd:y>280</pd:y>
        <config>
            <role>%%Constants/Logging/Roles/Debug%%</role>
        </config>
        <pd:inputBindings>
            <ns12:ActivityInput>
                <message>
                    <xsl:value-of select="tib:render-xml($JMS-Queue-Sender/ns03:aEmptyOutputClass, 1,0)"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Constants/Logging/Step/End"/>
                </msgCode>
            </ns12:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log - Start">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>552</pd:x>
        <pd:y>66</pd:y>
        <config>
            <role>%%Constants/Logging/Roles/Debug%%</role>
        </config>
        <pd:inputBindings>
            <ns12:ActivityInput>
                <message>
                    <xsl:value-of select="tib:render-xml($Start/pfx2:EMail/subject,1,1)"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Constants/Logging/Step/Start"/>
                </msgCode>
            </ns12:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Log - Start</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Publish EMail Service Request</pd:from>
        <pd:to>Log - Finished</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log - Start</pd:from>
        <pd:to>Publish EMail Service Request</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log - Finished</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:label>
        <pd:name>Label</pd:name>
        <pd:description>TechServices/EMail/Services/EMail</pd:description>
        <pd:x>149</pd:x>
        <pd:y>18</pd:y>
        <pd:width>297</pd:width>
        <pd:height>35</pd:height>
        <pd:fontName>Arial-bold-14</pd:fontName>
        <pd:fontColor>-16777216</pd:fontColor>
        <pd:color>-1</pd:color>
        <pd:type>solid</pd:type>
        <pd:thickness>0</pd:thickness>
        <pd:fade>true</pd:fade>
    </pd:label>
    <pd:label>
        <pd:name>Label (1)</pd:name>
        <pd:description>Very simple service that decouples sending emails from a process.

Since this is a service, files can not be attached as there is no
way to send the securitry context to the service to enable access
to a file.</pd:description>
        <pd:x>199</pd:x>
        <pd:y>76</pd:y>
        <pd:width>396</pd:width>
        <pd:height>89</pd:height>
        <pd:fontColor>-16777216</pd:fontColor>
        <pd:color>-1</pd:color>
        <pd:type>solid</pd:type>
        <pd:thickness>0</pd:thickness>
        <pd:fade>true</pd:fade>
    </pd:label>
</pd:ProcessDefinition>