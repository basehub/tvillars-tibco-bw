        <pd:y>62</pd:y>
        <config>
            <processName>/Services/Technical/EMail/Services/EMail.process</processName>
        </config>
        <pd:inputBindings>
            <ns:EMail>
                <from>
                    <xsl:value-of select="'tvillars@gmail.com'"/>
                </from>
                <to>
                    <xsl:value-of select="'tvillars@gmail.com'"/>
                </to>
                <cc>
                    <xsl:value-of select="'tvillars@gmail.com'"/>
                </cc>
                <subject>
                    <xsl:value-of select="'Unit Test For Email with 2  XML Attachments'"/>
                </subject>
                <bodyText>
                    <xsl:value-of select="'This is the body of the test message'"/>
                </bodyText>
                <XMLAttachment>
                    <xsl:attribute name="description">
