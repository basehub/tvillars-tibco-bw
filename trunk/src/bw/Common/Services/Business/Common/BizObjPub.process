<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" xmlns:pfx2="http://code.google.com/tvillars/tibco/bw/Common/Schemas/BizObjEnvelope" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:pfx3="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:ns3="http://uscis.gov/uscis/xsd/services/common/1.0" xmlns:ns4="http://www.tibco.com/pe/DeployedVarsType" xmlns:guid="java://com.chicos.eai.common.functions.GUID">
    <xsd:import namespace="http://code.google.com/tvillars/tibco/bw/Common/Schemas/BizObjEnvelope" schemaLocation="/Common/Schemas/BizObjEnvelope.xsd"/>
    <xsd:import namespace="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" schemaLocation="/Common/Functions/GEH.xsd"/>
    <pd:name>Services/Business/Common/BizObjPub.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="BizObjPub">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="JMSCorrelationID" type="xsd:string" minOccurs="0"/>
                    <xsd:choice>
                        <xsd:element name="Topic" type="xsd:string"/>
                        <xsd:element name="Queue" type="xsd:string"/>
                        <xsd:element name="MultipleQueues">
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="prefix" type="xsd:string" minOccurs="0"/>
                                    <xsd:choice>
                                        <xsd:element name="Queue" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                                        <xsd:element name="QueueCommaSep" type="xsd:string"/>
                                    </xsd:choice>
                                    <xsd:element name="suffix" type="xsd:string" minOccurs="0"/>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:choice>
                    <xsd:element ref="pfx2:BizObjEnvelope"/>
                    <xsd:element ref="pfx:RetryOptions"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>426</pd:startX>
    <pd:startY>45</pd:startY>
    <pd:returnBindings>
        <BizObjPub>
            <MessageID>
                <xsl:value-of select="$JMS-Topic-Publisher/ns2:aEmptyOutputClass/ns2:MessageID"/>
            </MessageID>
        </BizObjPub>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="BizObjPub">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="MessageID" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>632</pd:endX>
    <pd:endY>341</pd:endY>
    <pd:errorSchemas>
        <TechError ref="pfx:TechError"/>
    </pd:errorSchemas>
    <pd:processVariables>
        <Destinations>
            <xsd:element name="Destinations">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="destination" type="xsd:string" minOccurs="0" maxOccurs="unbounded"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Destinations>
    </pd:processVariables>
    <pd:group name="loop through ">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>37</pd:x>
        <pd:y>104</pd:y>
        <pd:width>515</pd:width>
        <pd:height>458</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <serializable>false</serializable>
            <pd:over>$Destinations/Destinations/destination</pd:over>
            <pd:iterationElementSlot>destination</pd:iterationElementSlot>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Publish Biz Object Envelope</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Log</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Publish Biz Object Envelope</pd:from>
            <pd:to>Log</pd:to>
            <pd:xpathDescription>sucess</pd:xpathDescription>
            <pd:lineType>Curved</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Publish Biz Object Envelope</pd:from>
            <pd:to>Gave Up (TechError)</pd:to>
            <pd:xpathDescription>gave up</pd:xpathDescription>
            <pd:lineType>Curved</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$GEH/GEH/GiveUp</pd:xpath>
        </pd:transition>
        <pd:group name="Publish Biz Object Envelope">
            <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
            <pd:resourceType>ae.process.group</pd:resourceType>
            <pd:x>77</pd:x>
            <pd:y>142</pd:y>
            <pd:width>256</pd:width>
            <pd:height>386</pd:height>
            <pd:collapsedWidth>57</pd:collapsedWidth>
            <pd:collapsedHeight>70</pd:collapsedHeight>
            <pd:groupVariables/>
            <pd:joinToggle>inherit</pd:joinToggle>
            <config>
                <pd:groupType>errorLoop</pd:groupType>
                <serializable>false</serializable>
                <pd:indexSlot>retryCount</pd:indexSlot>
                <pd:errorCondition>false()</pd:errorCondition>
                <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
            </config>
            <pd:inputBindings/>
            <pd:expanded>true</pd:expanded>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>JMS Topic Publisher</pd:to>
                <pd:xpathDescription>topic</pd:xpathDescription>
                <pd:lineType>Straight</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>xpath</pd:conditionType>
                <pd:xpath>exists($Start/BizObjPub/Topic)</pd:xpath>
            </pd:transition>
            <pd:transition>
                <pd:from>start</pd:from>
                <pd:to>JMS Queue Sender</pd:to>
                <pd:xpathDescription>queue</pd:xpathDescription>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>xpath</pd:conditionType>
                <pd:xpath>exists($Start/BizObjPub/Queue)
or
exists($Start/BizObjPub/MultipleQueues)</pd:xpath>
            </pd:transition>
            <pd:transition>
                <pd:from>JMS Queue Sender</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>JMS Topic Publisher</pd:from>
                <pd:to>end</pd:to>
                <pd:lineType>Straight</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>GEH</pd:from>
                <pd:to>end</pd:to>
                <pd:xpathDescription>give up</pd:xpathDescription>
                <pd:lineType>One Bend</pd:lineType>
                <pd:lineColor>-16777216</pd:lineColor>
                <pd:conditionType>always</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>JMS Topic Publisher</pd:from>
                <pd:to>GEH</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-65536</pd:lineColor>
                <pd:conditionType>error</pd:conditionType>
            </pd:transition>
            <pd:transition>
                <pd:from>JMS Queue Sender</pd:from>
                <pd:to>GEH</pd:to>
                <pd:lineType>Default</pd:lineType>
                <pd:lineColor>-65536</pd:lineColor>
                <pd:conditionType>error</pd:conditionType>
            </pd:transition>
            <pd:activity name="JMS Topic Publisher">
                <pd:type>com.tibco.plugin.jms.JMSTopicPublishActivity</pd:type>
                <pd:resourceType>ae.activities.JMSTopicPublishActivity</pd:resourceType>
                <pd:x>201</pd:x>
                <pd:y>219</pd:y>
                <config>
                    <PermittedMessageType>XML Text</PermittedMessageType>
                    <SessionAttributes>
                        <transacted>false</transacted>
                        <acknowledgeMode>1</acknowledgeMode>
                        <maxSessions>1</maxSessions>
                    </SessionAttributes>
                    <ConfigurableHeaders>
                        <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                        <JMSExpiration>0</JMSExpiration>
                        <JMSPriority>4</JMSPriority>
                    </ConfigurableHeaders>
                    <ConnectionReference>/Resources/JMS.Queue.sharedjmscon</ConnectionReference>
                    <InDataxsdString ref="pfx2:BizObjEnvelope"/>
                </config>
                <pd:inputBindings>
                    <ns2:ActivityInput>
                        <destinationTopic>
                            <xsl:value-of select="$destination/destination"/>
                        </destinationTopic>
                        <xsl:choose>
                            <xsl:when test="exists($Start/BizObjPub/JMSCorrelationID)">
                                <JMSCorrelationID>
                                    <xsl:value-of select="$Start/BizObjPub/JMSCorrelationID"/>
                                </JMSCorrelationID>
                            </xsl:when>
                            <xsl:otherwise>
                                <JMSCorrelationID>
                                    <xsl:value-of select="guid:makeGUID()"/>
                                </JMSCorrelationID>
                            </xsl:otherwise>
                        </xsl:choose>
                        <Body>
                            <xsl:copy-of select="$Start/BizObjPub/pfx2:BizObjEnvelope"/>
                        </Body>
                    </ns2:ActivityInput>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="GEH">
                <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
                <pd:resourceType>ae.process.subprocess</pd:resourceType>
                <pd:x>203</pd:x>
                <pd:y>341</pd:y>
                <config>
                    <processName>/Common/Functions/GEH.process</processName>
                </config>
                <pd:inputBindings>
                    <GEH>
                        <xsl:copy-of select="$_error/ns1:ErrorReport"/>
                        <ns3:Context>
                            <ns3:ProcessInput>
                                <xsl:copy-of select="$Start/BizObjPub"/>
                            </ns3:ProcessInput>
                        </ns3:Context>
                        <Retry>
                            <RetryCount>
                                <xsl:value-of select="$retryCount"/>
                            </RetryCount>
                            <xsl:copy-of select="$Start/BizObjPub/pfx:RetryOptions"/>
                        </Retry>
                    </GEH>
                </pd:inputBindings>
            </pd:activity>
            <pd:activity name="JMS Queue Sender">
                <pd:type>com.tibco.plugin.jms.JMSQueueSendActivity</pd:type>
                <pd:resourceType>ae.activities.JMSQueueSendActivity</pd:resourceType>
                <pd:x>199</pd:x>
                <pd:y>444</pd:y>
                <config>
                    <PermittedMessageType>XML Text</PermittedMessageType>
                    <SessionAttributes>
                        <transacted>false</transacted>
                        <acknowledgeMode>1</acknowledgeMode>
                        <maxSessions>1</maxSessions>
                    </SessionAttributes>
                    <ConfigurableHeaders>
                        <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                        <JMSExpiration>0</JMSExpiration>
                        <JMSPriority>4</JMSPriority>
                    </ConfigurableHeaders>
                    <ConnectionReference>/Resources/JMS.Queue.sharedjmscon</ConnectionReference>
                    <InDataxsdString ref="pfx2:BizObjEnvelope"/>
                </config>
                <pd:inputBindings>
                    <ns2:ActivityInput>
                        <destinationQueue>
                            <xsl:value-of select="$destination/destination"/>
                        </destinationQueue>
                        <xsl:choose>
                            <xsl:when test="exists($Start/BizObjPub/JMSCorrelationID)">
                                <JMSCorrelationID>
                                    <xsl:value-of select="$Start/BizObjPub/JMSCorrelationID"/>
                                </JMSCorrelationID>
                            </xsl:when>
                            <xsl:otherwise>
                                <JMSCorrelationID>
                                    <xsl:value-of select="guid:makeGUID()"/>
                                </JMSCorrelationID>
                            </xsl:otherwise>
                        </xsl:choose>
                        <Body>
                            <xsl:copy-of select="$Start/BizObjPub/pfx2:BizObjEnvelope"/>
                        </Body>
                    </ns2:ActivityInput>
                </pd:inputBindings>
            </pd:activity>
        </pd:group>
        <pd:activity name="Gave Up (TechError)">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>493</pd:x>
            <pd:y>395</pd:y>
            <config>
                <faultName>TechError</faultName>
            </config>
            <pd:inputBindings>
                <ns:ActivityInput>
                    <message>
                        <xsl:value-of select="$GEH/GEH/GEHInput/ns1:ErrorReport/Msg"/>
                    </message>
                    <xsl:if test="$GEH/GEH/GEHInput/ns1:ErrorReport/MsgCode">
                        <messageCode>
                            <xsl:value-of select="$GEH/GEH/GEHInput/ns1:ErrorReport/MsgCode"/>
                        </messageCode>
                    </xsl:if>
                    <data>
                        <pfx:TechError>
                            <ns3:Context>
                                <ns3:ProcessInput>
                                    <xsl:copy-of select="$Start/BizObjPub"/>
                                </ns3:ProcessInput>
                                <ns3:ActivityOutputs>
                                    <xsl:copy-of select="$destination/destination"/>
                                </ns3:ActivityOutputs>
                                <ns3:ActivityOutputs>
                                    <xsl:copy-of select="$Destinations/Destinations"/>
                                </ns3:ActivityOutputs>
                            </ns3:Context>
                            <pfx:DescShort>
                                <xsl:value-of select="$GEH/GEH/GEHInput/DescShort"/>
                            </pfx:DescShort>
                            <xsl:for-each select="$GEH/GEH/GEHInput/DescLong">
                                <pfx:DescLong>
                                    <xsl:value-of select="."/>
                                </pfx:DescLong>
                            </xsl:for-each>
                            <pfx:ErrorReport>
                                <xsl:copy-of select="$GEH/GEH/GEHInput/ns1:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                                <xsl:copy-of select="$GEH/GEH/GEHInput/ns1:ErrorReport/@*"/>
                                <xsl:copy-of select="$GEH/GEH/GEHInput/ns1:ErrorReport/node()"/>
                            </pfx:ErrorReport>
                            <xsl:for-each select="$GEH/GEH/GEHInput/Retry">
                                <pfx:Retry>
                                    <pfx:RetryCount>
                                        <xsl:value-of select="RetryCount"/>
                                    </pfx:RetryCount>
                                    <xsl:copy-of select="pfx:RetryOptions"/>
                                </pfx:Retry>
                            </xsl:for-each>
                        </pfx:TechError>
                    </data>
                </ns:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Log">
            <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
            <pd:resourceType>ae.activities.log</pd:resourceType>
            <pd:x>455</pd:x>
            <pd:y>235</pd:y>
            <config>
                <role>%%Constants/Logging/Roles/Info%%</role>
            </config>
            <pd:inputBindings>
                <pfx3:ActivityInput>
                    <message>
                        <xsl:value-of select="tib:render-xml($Start/BizObjPub/pfx2:BizObjEnvelope/pfx2:BizObjHeader, 1,0)"/>
                    </message>
                    <msgCode>
                        <xsl:value-of select="$_globalVariables/ns4:GlobalVariables/Constants/Logging/Step/End"/>
                    </msgCode>
                </pfx3:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Calc Destinations">
        <pd:type>com.tibco.pe.core.AssignActivity</pd:type>
        <pd:resourceType>ae.activities.assignActivity</pd:resourceType>
        <pd:x>518</pd:x>
        <pd:y>45</pd:y>
        <config>
            <variableName>Destinations</variableName>
        </config>
        <pd:inputBindings>
            <Destinations>
                <xsl:choose>
                    <xsl:when test="exists($Start/BizObjPub/Topic)">
                        <destination>
                            <xsl:value-of select="$Start/BizObjPub/Topic"/>
                        </destination>
                    </xsl:when>
                    <xsl:when test="exists($Start/BizObjPub/Queue)">
                        <xsl:if test="$Start/BizObjPub/Queue">
                            <destination>
                                <xsl:value-of select="$Start/BizObjPub/Queue"/>
                            </destination>
                        </xsl:if>
                    </xsl:when>
                    <xsl:when test="exists($Start/BizObjPub/MultipleQueues)">
                        <xsl:for-each select="$Start/BizObjPub/MultipleQueues/Queue&#xA;|&#xA;tib:tokenize($Start/BizObjPub/MultipleQueues/QueueCommaSep, ',')">
                            <destination>
                                <xsl:value-of select="concat(&#xA;  if (exists($Start/BizObjPub/MultipleQueues/prefix))&#xA;    then concat($Start/BizObjPub/MultipleQueues/prefix,'.')&#xA;    else ''&#xA;,.&#xA;,  if (exists($Start/BizObjPub/MultipleQueues/suffix))&#xA;    then concat('.',$Start/BizObjPub/MultipleQueues/suffix)&#xA;    else ''&#xA;)"/>
                            </destination>
                        </xsl:for-each>
                    </xsl:when>
                    <xsl:otherwise>
                        <destination>
                            <xsl:value-of select="'BizService.DeadLetter'"/>
                        </destination>
                    </xsl:otherwise>
                </xsl:choose>
            </Destinations>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Calc Destinations</pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>loop through </pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Calc Destinations</pd:from>
        <pd:to>loop through </pd:to>
        <pd:lineType>Multiple Bends</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:label>
        <pd:name>Label (1)</pd:name>
        <pd:description>Publishes a business object envelope to the specified
topic or queue(s) and the specfied retry option.

All Biz Service publisher's should use this process to place a Biz
Object on the Enterprise Service Bus.</pd:description>
        <pd:x>195</pd:x>
        <pd:y>48</pd:y>
        <pd:width>381</pd:width>
        <pd:height>95</pd:height>
        <pd:fontColor>-16777216</pd:fontColor>
        <pd:color>-1</pd:color>
        <pd:type>solid</pd:type>
        <pd:thickness>0</pd:thickness>
        <pd:fade>true</pd:fade>
    </pd:label>
</pd:ProcessDefinition>