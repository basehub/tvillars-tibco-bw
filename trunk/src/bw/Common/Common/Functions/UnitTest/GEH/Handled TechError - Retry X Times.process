<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:guid="java://com.chicos.eai.common.functions.GUID" xmlns:ns="http://www.tibco.com/pe/EngineTypes" xmlns:ns2="http://uscis.gov/uscis/xsd/services/common/1.0" xmlns:ns0="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns3="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" xmlns:ns32="http://www.tibco.com/namespaces/tnt/plugins/file/v2">
    <xsd:import namespace="http://code.google.com/tvillars/tibco/bw/Common/Functions/GEH" schemaLocation="/Common/Functions/GEH.xsd"/>
    <pd:name>Common/Functions/UnitTest/GEH/Handled TechError - Retry X Times.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="Start">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="FileName" type="xsd:string" minOccurs="0"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>66</pd:startX>
    <pd:startY>173</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>734</pd:endX>
    <pd:endY>52</pd:endY>
    <pd:errorSchemas>
        <TechError ref="ns3:TechError"/>
        <DataError ref="ns3:DataError"/>
    </pd:errorSchemas>
    <pd:processVariables/>
    <pd:group name="read file - If error, log, pause and retry up to 3 times">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>137</pd:x>
        <pd:y>1</pd:y>
        <pd:width>312</pd:width>
        <pd:height>324</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <serializable>false</serializable>
            <pd:indexSlot>retryCount</pd:indexSlot>
            <pd:errorCondition>$GEH---Read-File/GEH/GiveUp = "true"</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Read File</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>GEH - Read File</pd:from>
            <pd:to>end</pd:to>
            <pd:xpathDescription/>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>GEH - Read File</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65536</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:activity name="GEH - Read File">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>277</pd:x>
            <pd:y>234</pd:y>
            <config>
                <processName>/Common/Functions/GEH.process</processName>
            </config>
            <pd:inputBindings>
                <GEH>
                    <xsl:copy-of select="$_error/ns:ErrorReport"/>
                    <ns2:Context>
                        <ns2:ProcessInput>
                            <xsl:copy-of select="$Start/Start"/>
                        </ns2:ProcessInput>
                    </ns2:Context>
                    <Retry>
                        <RetryCount>
                            <xsl:value-of select="$retryCount"/>
                        </RetryCount>
                        <ns3:RetryOptions>
                            <ns3:TryXTimes>
                                <ns3:TimesToTry>
                                    <xsl:value-of select="3"/>
                                </ns3:TimesToTry>
                                <ns3:SecsToPauseBetweenRetries>
                                    <xsl:value-of select="1"/>
                                </ns3:SecsToPauseBetweenRetries>
                            </ns3:TryXTimes>
                        </ns3:RetryOptions>
                    </Retry>
                </GEH>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Read File">
            <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
            <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
            <pd:x>276</pd:x>
            <pd:y>66</pd:y>
            <config>
                <encoding>text</encoding>
            </config>
            <pd:inputBindings>
                <ns32:ReadActivityInputClass>
                    <fileName>
                        <xsl:value-of select="$Start/Start/FileName"/>
                    </fileName>
                </ns32:ReadActivityInputClass>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Give Up Reading File">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>604</pd:x>
        <pd:y>287</pd:y>
        <config>
            <faultName>TechError</faultName>
        </config>
        <pd:inputBindings>
            <ns0:ActivityInput>
                <data>
                    <ns3:TechError>
                        <xsl:copy-of select="$GEH---Read-File/GEH/GEHInput/ns2:Context"/>
                        <ns3:DescShort>
                            <xsl:value-of select="$GEH---Read-File/GEH/GEHInput/DescShort"/>
                        </ns3:DescShort>
                        <xsl:for-each select="$GEH---Read-File/GEH/GEHInput/DescLong">
                            <ns3:DescLong>
                                <xsl:value-of select="."/>
                            </ns3:DescLong>
                        </xsl:for-each>
                        <ns3:ErrorReport>
                            <xsl:copy-of select="$_error/ns:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                            <xsl:copy-of select="$_error/ns:ErrorReport/@*"/>
                            <xsl:copy-of select="$_error/ns:ErrorReport/node()"/>
                        </ns3:ErrorReport>
                        <xsl:for-each select="$GEH---Read-File/GEH/GEHInput/Retry">
                            <ns3:Retry>
                                <ns3:RetryCount>
                                    <xsl:value-of select="RetryCount"/>
                                </ns3:RetryCount>
                                <xsl:copy-of select="ns3:RetryOptions"/>
                            </ns3:Retry>
                        </xsl:for-each>
                    </ns3:TechError>
                </data>
            </ns0:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Parse XML">
        <pd:type>com.tibco.plugin.xml.XMLParseActivity</pd:type>
        <pd:resourceType>ae.activities.XMLParseActivity</pd:resourceType>
        <pd:x>604</pd:x>
        <pd:y>52</pd:y>
        <config>
            <inputStyle>text</inputStyle>
            <xsdVersion>default</xsdVersion>
            <validateOutput>true</validateOutput>
            <term>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="param" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </term>
        </config>
        <pd:inputBindings>
            <xmlString>
                <xsl:value-of select="$Read-File/ns1:ReadActivityOutputTextClass/fileContent/textContent"/>
            </xmlString>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Parsing Error">
        <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>604</pd:x>
        <pd:y>160</pd:y>
        <config>
            <faultName>DataError</faultName>
        </config>
        <pd:inputBindings>
            <ns0:ActivityInput>
                <data>
                    <ns3:DataError>
                        <ns2:Context>
                            <ns2:ProcessInput>
                                <xsl:copy-of select="$Start/Start"/>
                            </ns2:ProcessInput>
                            <ns2:ActivityOutputs>
                                <xsl:if test="$Read-File/ns1:ReadActivityOutputTextClass/fileInfo/fileName">
                                    <xsl:attribute name="description">
                                        <xsl:value-of select="$Read-File/ns1:ReadActivityOutputTextClass/fileInfo/fileName"/>
                                    </xsl:attribute>
                                </xsl:if>
                                <xsl:copy-of select="$Read-File/ns1:ReadActivityOutputTextClass"/>
                            </ns2:ActivityOutputs>
                        </ns2:Context>
                        <ns3:DescShort>
                            <xsl:value-of select="'Unable to parse'"/>
                        </ns3:DescShort>
                        <ns3:ErrorReport>
                            <xsl:copy-of select="$_error/ns:ErrorReport/ancestor-or-self::*/namespace::node()"/>
                            <xsl:copy-of select="$_error/ns:ErrorReport/@*"/>
                            <xsl:copy-of select="$_error/ns:ErrorReport/node()"/>
                        </ns3:ErrorReport>
                    </ns3:DataError>
                </data>
            </ns0:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>read file - If error, log, pause and retry up to 3 times</pd:from>
        <pd:to>Parse XML</pd:to>
        <pd:lineType>Curved</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>read file - If error, log, pause and retry up to 3 times</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>read file - If error, log, pause and retry up to 3 times</pd:from>
        <pd:to>Give Up Reading File</pd:to>
        <pd:xpathDescription>give up</pd:xpathDescription>
        <pd:lineType>Curved</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$GEH---Read-File/GEH/GiveUp</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>Parsing Error</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse XML</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>